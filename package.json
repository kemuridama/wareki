{
  "name": "wareki",
  "version": "0.0.6",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --extensions \".ts\" --out-dir lib",
    "check": "tsc",
    "test": "ava",
    "test:cov": "nyc ava && nyc report | coveralls",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "prepublishOnly": "build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tohashi/wareki.git"
  },
  "author": "tohashi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tohashi/wareki/issues"
  },
  "homepage": "https://github.com/tohashi/wareki#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-typescript": "^7.0.0-rc.1",
    "@babel/register": "^7.0.0-rc.1",
    "ava": "1.0.0-beta.6",
    "coveralls": "^3.0.2",
    "nyc": "^12.0.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "files": [
    "lib"
  ],
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "include": [
      "./src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "html",
      "text-lcov"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": false,
    "all": true
  }
}
